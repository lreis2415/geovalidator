@prefix arcgis: <http://www.egc.org/ont/process/arcgis#> .
@prefix data: <http://www.egc.org/ont/data#> .
@prefix dta: <http://www.egc.org/ont/domain/dta#> .
@prefix gcmd: <https://gcmdservices.gsfc.nasa.gov/kms/concept/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix ogc: <http://www.egc.org/ont/vocab/ogc#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sf: <http://www.opengis.net/ont/sf#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix taudem: <http://www.egc.org/ont/process/taudem#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

data:InputElevationShape a sh:NodeShape ;
    sh:node data:DEMDataShape ;
    sh:property [ sh:class data:GDALRasterFormat ;
            sh:defaultValue data:GTiff ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path data:dataFormat ] ;
    sh:targetNode taudem:Input_DEM_Dataset,
        taudem:Input_Elevation_Grid .

data:DEMDataShape a sh:NodeShape ;
    sh:property [ sh:in ( gcmd:dem dta:DEM ogc:DigitalElevationModel ) ;
            sh:maxCount 1 ;
            sh:message "Only supports Digital Elevation Model (DEM) data"@en ;
            sh:minCount 1 ;
            sh:path data:dataTheme ] .

