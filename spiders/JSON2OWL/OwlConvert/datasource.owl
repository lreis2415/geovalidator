    <!-- http://www.egc.org/ont/datasource#ClassTable -->

    <owl:Class rdf:about="http://www.egc.org/ont/datasource#ClassTable">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.egc.org/ont/base/props#hasNumericValue"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.egc.org/ont/datasource#hasColor"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.egc.org/ont/datasource#ClimaticDataSet -->

    <owl:Class rdf:about="http://www.egc.org/ont/datasource#ClimaticDataSet">
        <rdfs:subClassOf rdf:resource="http://www.egc.org/ont/datasource#RasterDataSet"/>
    </owl:Class>
    


    <!-- http://www.egc.org/ont/datasource#DEMDataSet -->

    <owl:Class rdf:about="http://www.egc.org/ont/datasource#DEMDataSet">
        <owl:equivalentClass rdf:resource="http://www.egc.org/ont/datasource#ElevationDataSet"/>
        <rdfs:subClassOf rdf:resource="http://www.egc.org/ont/datasource#RasterDataSet"/>
    </owl:Class>
    


    <!-- http://www.egc.org/ont/datasource#DataProvider -->

    <owl:Class rdf:about="http://www.egc.org/ont/datasource#DataProvider">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.egc.org/ont/base/props#hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.egc.org/ont/datasource#DataSet"/>
    </owl:Class>
    


    <!-- http://www.egc.org/ont/datasource#DataSet -->

    <owl:Class rdf:about="http://www.egc.org/ont/datasource#DataSet">
        <owl:disjointWith rdf:resource="http://www.egc.org/ont/datasource#DataSource"/>
    </owl:Class>
    


