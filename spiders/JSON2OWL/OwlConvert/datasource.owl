        <props:abbreviationOf xml:lang="en">Advanced Spaceborne Thermal Emission and Reflection Radiometer</props:abbreviationOf>
        <skos:altLabel xml:lang="en">Advanced Spaceborne Thermal Emission and Reflection Radiometer</skos:altLabel>
