<Geostatistic rdf:about="#Multi_Band_Variation_task">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <processingTool rdf:resource="http://www.egc.org/ont/process/saga#Multi_Band_Variation"/>
  <term:description xml:lang="en">Calculates for each cell the spectral variation based on feature space distances to the centroid for all cells in specified neighbourhood. The average distance has been used for Spectral Variation Hypothesis (SVH).</term:description>
