#    num_proc = 2
#    wp = '../tests/data/tmp_results/wtsd_delineation'
#
#    TauDEMWorkflow.watershed_delineation(num_proc, dem, workingdir=wp)

vocab:DEM a skos:Concept.

process:Parameter a rdfs:Class.
process:GeoprocessingTool a rdfs:Class.
process:hasData a owl:ObjectProperty  .

pygeoc:watershed_delineation_dem a process:Parameter.
pygeoc:watershed_delineation_num_proc a process:Parameter.

pygeoc:SeimsPreprocessingTool rdfs:subClassOf process:GeoprocessingTool.

pygeoc:watershed_delineation a pygeoc:SeimsPreprocessingTool;
        process:hasInput pygeoc:watershed_delineation_dem;
        process:hasInput pygeoc:watershed_delineation_num_proc.

#---------------------------------------------------
#             data graphs
#---------------------------------------------------

pygeoc:watershed_delineation_num_proc
	dcterms:identifier "num_proc"^^xsd:string;
    process:hasLiteralData "3"^^xsd:int ;
.

pygeoc:watershed_delineation_dem
	dcterms:identifier "dem"^^xsd:string;
    process:hasData [
       a data:RasterData ;
       data:dataFormat data:GTiff ;
       data:dataContent "ywzdem30m.tif" ;
       # error
       data:isProjected false ;
       data:hasEPSG "2415"^^xsd:int ;
       dcat:theme theme:el,
                gcmd:digital_elevation_models,
                gemet:10140 ;
    ];
.
